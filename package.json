{
  "dependencies": {
    "icon-font-generator": "^2.1.10",
    "svgo": "1.2.2"
  },
  "scripts": {
    "build:svgclean": "node scripts/svgcleaner.js",
    "build:font": "./node_modules/icon-font-generator/bin/icon-font-generator svgs/*.svg -o build -n fontaudio",
    "build:jucedata": "node scripts/makejucecode.js",
    "build:all": "yarn build:svgclean && yarn build:font && yarn build:jucedata && open ./build/fontaudio.html"
  }
}
